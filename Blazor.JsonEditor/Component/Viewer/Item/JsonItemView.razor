@{
    if (Component != null)
    {
        <DynamicComponent Type="Component" Parameters="DynamicComponentParameters"></DynamicComponent>
    }
    else
    {
        <JsonItemDefaultView JsonItem="JsonItem" ValueChanged="ValueChanged" AllowEdit="@AllowEdit" CustomEditor="CustomEditor" JsonObject="JsonObject" KeyValues="KeyValues" CustomItemView="CustomItemView" CustomObjectView="CustomObjectView"></JsonItemDefaultView>
    }
}